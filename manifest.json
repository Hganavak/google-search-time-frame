{
    "manifest_version": 2,
  
    "name": "Google Search Time Frame",
    "description": "This extension sets the search time frame to the last year.",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "images/get_started128.png",
      "default_title": "Google Search Time Frame"
    },
    
    "background":{
      "scripts":["background.js"]
    },
  
    "permissions": [
      "activeTab",
      "tabs"
    ],

    "author": "Sam Kavanagh (tofu.dev)"
  }